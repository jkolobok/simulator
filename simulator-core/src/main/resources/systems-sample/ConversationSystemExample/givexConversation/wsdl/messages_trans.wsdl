      </complexType>
     
      <complexType name="OrderCertCancelInfoElement">
          <sequence>
           <element name="givexNumber"      type="xsd:string" />
           <element name="certBalance"      type="xsd:decimal" />
         </sequence>
      </complexType>

      <simpleType name="HistoryType">
        <restriction base="xsd:string">
          <enumeration value="Certificate" />
          <enumeration value="PreAuth" />
          <enumeration value="Events" />
          <enumeration value="Points" />
        </restriction>
      </simpleType>

      <element name="GetBalance">
        <complexType>
          <sequence>
            <element name="id"           type="gvxCommon:Identification" />
            <element name="givexNumber"  type="xsd:string" />
