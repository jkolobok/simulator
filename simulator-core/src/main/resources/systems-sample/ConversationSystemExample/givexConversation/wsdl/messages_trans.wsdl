          <element name="tier"         type="xsd:string" />
          <element name="authCode"     type="xsd:string" />
          <element name="activeFlag"   type="xsd:boolean" />
          <element name="channelId"    type="xsd:long" />
        </sequence>
      </complexType>
  
      <complexType name="OrderInfoElement">
         <sequence>
           <element name="orderAddressID"   type="xsd:int" />
           <element name="orderAmount"      type="xsd:decimal" />
         </sequence>
      </complexType>
     
      <complexType name="OrderCertInfoElement">
         <sequence>
           <element name="orderAddressID"   type="xsd:int" />
           <element name="givexNumber"      type="xsd:string" />
           <element name="securityCode"     type="xsd:string" minOccurs="0" />
