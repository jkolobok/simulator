      <complexType name="CertTransactionElement">
        <sequence>
          <element name="transId"      type="xsd:long" />
          <element name="user"         type="xsd:string" />
          <element name="oper"         type="xsd:string" />
          <element name="transType"    type="xsd:string" />
          <element name="date"         type="xsd:date" />
          <element name="amount"       type="xsd:decimal" />
          <element name="authCode"     type="xsd:string" />
          <element name="activeFlag"   type="xsd:boolean" />
          <element name="channelId"    type="xsd:long" />
        </sequence>
      </complexType>

      <complexType name="PreAuthTransactionElement">
        <sequence>
          <element name="transId"      type="xsd:long" />
          <element name="user"         type="xsd:string" />
          <element name="oper"         type="xsd:string" />
          <element name="date"         type="xsd:date" />
          <element name="amount"       type="xsd:decimal" />
          <element name="authCode"     type="xsd:string" />
          <element name="activeFlag"   type="xsd:boolean" />
          <element name="channelId"    type="xsd:long" />
        </sequence>
      </complexType>

      <complexType name="PointsTransactionElement">
        <sequence>
          <element name="transId"      type="xsd:long" />
          <element name="user"         type="xsd:string" />
          <element name="oper"         type="xsd:string" />
          <element name="transType"    type="xsd:string" />
          <element name="date"         type="xsd:date" />
          <element name="dateVisit"    type="xsd:date" />
          <element name="amount"       type="xsd:decimal" />
          <element name="units"        type="xsd:long" />
